package vo.member;

public class MemberCreateDto {
	private String meberId;
	private String memberPw;
	
	public MemberCreateDto() {
	}
	private MemberCreateDto(Builder builder) {
		this.meberId = builder.meberId;
		this.memberPw = builder.memberPw;
	}
	
	public String getMeberId() {
		return meberId;
	}
	public void setMeberId(String meberId) {
		this.meberId = meberId;
	}
	public String getMemberPw() {
		return memberPw;
	}
	public void setMemberPw(String memberPw) {
		this.memberPw = memberPw;
	}
	
	@Override
	public String toString() {
		return "MemberCreateDto [meberId=" + meberId + ", memberPw=" + memberPw + "]";
	}
	
	//builder
	public static class Builder {
		private String meberId;
		private String memberPw;
		
		public MemberCreateDto build() {
			return new MemberCreateDto(this);
		}
		
		public static Builder meberId(String meberId) {
			this.meberId = meberId;
			return this;
		}
		public Builder memberPw(String memberPw) {
			this.memberPw = memberPw;
			return this;
		}
		
	}
}
